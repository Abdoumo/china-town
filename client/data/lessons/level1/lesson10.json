  "dialogue": {
    "title": "Time and Schedules",
    "lines": [
      {
        "speaker": "Xiaoyè",
        "chinese": "现在几点？",
        "pinyin": "Xiànzài jǐ diǎn?",
        "english": "What time is it now?"
      },
      {
        "speaker": "Lili",
        "chinese": "现在七点一刻。",
        "pinyin": "Xiànzài qī diǎn yī kè.",
        "english": "It's a quarter past seven now."
      },
      {
        "speaker": "Xiaoyè",
        "chinese": "你几点吃早饭？",
        "pinyin": "Nǐ jǐ diǎn chī zǎofàn?",
        "english": "What time do you eat breakfast?"
      },
      {
        "speaker": "Lili",
        "chinese": "我七点半吃早饭。",
        "pinyin": "Wǒ qī diǎn bàn chī zǎofàn.",
        "english": "I eat breakfast at half past seven."
      },
      {
        "speaker": "Xiaoyè",
        "chinese": "星期天咱们去颐和园，怎么样？",
        "pinyin": "Xīngqītiān zánmen qù Yíhéyuán, zěnmeyàng?",
        "english": "Let's go to the Summer Palace on Sunday, how about it?"
      },
      {
        "speaker": "Lili",
        "chinese": "好啊。什么时候出发？",
        "pinyin": "Hǎo a. Shénme shíhou chūfā?",
        "english": "Great. When shall we leave?"
      }
    ]
  },
  "grammar": [
    {
      "point": "Telling Time",
      "explanation": "Structure: [Hour] + 点 + [Minutes]. For 15 min: 一刻, for 30 min: 半, for other: 分.",
      "example": "八点、八点十五分/八点一刻、八点三十分/八点半、差五分八点",
      "translation": "8:00, 8:15, 8:30, 7:55"
    },
    {
      "point": "Date Expression",
      "explanation": "Structure: [Year] + 年 + [Month] + 月 + [Day] + 号/日 + [Day of week] + 星期",
      "example": "二零二四年七月二十二号星期三",
      "translation": "July 22, 2024, Wednesday"
    },
    {
      "point": "Time Ranges",
      "explanation": "Use 从...到... to express time duration. Structure: 从 + [start] + 到 + [end]",
      "example": "从上午八点到十二点。",
      "translation": "From 8am to 12pm."
    },
    {
      "point": "Particle 吧",
      "explanation": "吧 softens statements or seeks confirmation. It shows uncertainty or invites agreement.",
      "example": "是星期天吧？ 咱们去吧。",
      "translation": "It's Sunday, right? Let's go."
    }
  ],
